
.row {
  padding: 0;
  margin: 0;
  height: 25px;
}

.cell {
  display: inline-block;
  width: 25px;
  height: 25px;
  padding: 0;
  margin: 0;

  &.empty {
    background-color: black;
  }

  &.bush {
    background-color: green;
  }
  
  &.grass {
    background-color: lightgreen;
  }
  
  &.tree {
    background-color: darkgreen;
  }
}
